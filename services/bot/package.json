{
  "name": "qutex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "src/@types/index.d.ts",
  "scripts": {
    "test": "jest -i --coverage --testLocationInResults",
    "start:dev": "nodemon",
    "build": "rimraf build && tsc",
    "start": "node build/index.js",
    "lint": "eslint .",
    "qa": "yarn lint && yarn test",
    "start:up": "ts-node src/startup/startup.ts",
    "start:dev-bot": "yarn stop:ngrok; yarn start:ngrok && yarn start:up",
    "start:ngrok": "ts-node src/startup/ngrok.ts &",
    "stop:ngrok": "ps aux | grep ngrok | grep -v grep | grep -v yarn | awk '{print $2}' | xargs kill -9",
    "migrate:create": "migrate-mongo new",
    "migrate:status": "migrate-mongo status",
    "migrate:undo-last": "migrate-mongo down",
    "migrate": "migrate-mongo up"
  },
  "keywords": [
    "queue",
    "mutex",
    "semaphores"
  ],
  "author": "Ava Thorn",
  "license": "ISC",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js,.d.ts",
    "ignore": [],
    "exec": "ts-node src/index.ts"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios": "^0.21.1",
    "babel-jest": "^26.0.22",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "mrm-task-npm-docker": "^1.2.0",
    "ngrok": "^4.0.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.16.4",
    "migrate-mongo": "^8.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.5",
    "webex": "1.119.4",
    "winston": "^3.3.3"
  }
}
